{
    "rules": {
        "type != 'EmergencyDelete'": {
            "actions": [
                "_parse_emergency_on"
            ],
            "matchable": "model_object",
            "rules": {
                "type == 'EmergencyAlert'": {
                    "actions": [
                        "SaveEmergency",
                        "EmergencyBroadcast"
                    ]
                },
                "type == 'EmergencyInContact'": {
                    "actions": [
                        "SaveEmergency",
                        "EmergencyBroadcast"
                    ]
                },
                "type == 'EmergencyRingTheBell'": {
                    "actions": [
                        "SaveEmergency",
                        "EmergencyBroadcast"
                    ]
                },
                "type == 'EmergencyGeoFenceBreached'": {
                    "actions": [
                        "SaveEmergency",
                        "EmergencyBroadcast"
                    ]
                },
                "type == 'EmergencyCancelled'": {
                    "actions": [
                        "SaveEmergency",
                        "EmergencyBroadcast"
                    ]
                }
            }
        },
        "type == 'EmergencyDelete'": {
            "actions": [
                "_parse_emergency_off"
            ],
            "matchable": "model_object",
            "rules": {
                "type == 'EmergencyDelete'": {
                    "actions": [
                        "_remove_emergency_from_emergencies",
                        "EmergencyBroadcast"
                    ]
                }
            }
        },
        "type == 'ClientConnected'": {
            "actions": [
                "EmergencyBroadcast_all"
            ]
        }
    },
    "matchable": "xml_element"
}